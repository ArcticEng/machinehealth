{
  "name": "monitoring-app-backend",
  "version": "1.0.0",
  "description": "Backend API for Machine Monitoring Application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "node dist/db/migrate.js",
